{
    "database": {
        "rules": "database.rules.json"
    },
    "firestore": [
        {
            "database": "sishu",
            "rules": "firestore.rules"
        }
    ],
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "ignore": [
                "node_modules",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log"
            ],
            "predeploy": [
                "npm --prefix \"$RESOURCE_DIR\" run build"
            ]
        }
    ],
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [
            {
                "source": "**/*.@(js|css|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2|ttf)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public,max-age=31536000,immutable"
                    }
                ]
            },
            {
                "source": "/index.html",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public,max-age=0,must-revalidate"
                    }
                ]
            },
            {
                "source": "/flutter_service_worker.js",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "public,max-age=0,must-revalidate"
                    }
                ]
            }
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "sishu-4bbfb",
                    "appId": "1:745836365623:android:60a0ede29dcf5facc63cb2",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "ios": {
                "default": {
                    "projectId": "sishu-4bbfb",
                    "appId": "1:745836365623:ios:fe5726bb4aa70f65c63cb2",
                    "uploadDebugSymbols": false,
                    "fileOutput": "ios/Runner/GoogleService-Info.plist"
                }
            },
            "macos": {
                "default": {
                    "projectId": "sishu-4bbfb",
                    "appId": "1:745836365623:ios:fe5726bb4aa70f65c63cb2",
                    "uploadDebugSymbols": false,
                    "fileOutput": "macos/Runner/GoogleService-Info.plist"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "sishu-4bbfb",
                    "configurations": {
                        "android": "1:745836365623:android:60a0ede29dcf5facc63cb2",
                        "ios": "1:745836365623:ios:fe5726bb4aa70f65c63cb2",
                        "macos": "1:745836365623:ios:fe5726bb4aa70f65c63cb2",
                        "web": "1:745836365623:web:7f544412620369e1c63cb2"
                    }
                }
            }
        }
    }
}
